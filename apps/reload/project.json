{
  "name": "reload",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/reload/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nx/react-native:start",
      "options": {
        "port": 8081
      },
      "dependsOn": [
        "ensure-symlink",
        "sync-deps",
        "pod-install"
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx start reload"
      }
    },
    "patch-ios-incremental-build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "touch apps/reload/ios/.xcode.env"
      }
    },
    "run-ios": {
      "dependsOn": [
        "ensure-symlink",
        "sync-deps",
        "pod-install",
        "patch-ios-incremental-build"
      ],
      "executor": "@nx/react-native:run-ios",
      "options": {}
    },
    "bundle-ios": {
      "executor": "@nx/react-native:bundle",
      "outputs": [
        "{options.bundleOutput}"
      ],
      "options": {
        "entryFile": "index.js",
        "platform": "ios",
        "bundleOutput": "dist/apps/reload/ios/main.jsbundle"
      },
      "dependsOn": [
        "ensure-symlink"
      ]
    },
    "run-android": {
      "executor": "@nx/react-native:run-android",
      "options": {},
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "build-android": {
      "executor": "@nx/react-native:build-android",
      "outputs": [
        "{projectRoot}/android/app/build/outputs/bundle",
        "{projectRoot}/android/app/build/outputs/apk"
      ],
      "options": {},
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "build-ios": {
      "executor": "@nx/react-native:build-ios",
      "outputs": [
        "{projectRoot}/ios/build/Build"
      ],
      "options": {},
      "dependsOn": [
        "ensure-symlink",
        "sync-deps",
        "pod-install"
      ]
    },
    "pod-install": {
      "executor": "@nx/react-native:pod-install",
      "options": {},
      "dependsOn": [
        "ensure-symlink",
        "sync-deps"
      ]
    },
    "bundle-android": {
      "executor": "@nx/react-native:bundle",
      "outputs": [
        "{options.bundleOutput}"
      ],
      "options": {
        "entryFile": "index.js",
        "platform": "android",
        "bundleOutput": "dist/apps/reload/android/main.jsbundle"
      },
      "dependsOn": [
        "ensure-symlink"
      ]
    },
    "sync-deps": {
      "executor": "@nx/react-native:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nx/react-native:ensure-symlink",
      "options": {}
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/reload/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/reload/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}